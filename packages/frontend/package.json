{
  "name": "@news-map/frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:all": "tsx src/__tests__/test-runner.ts all",
    "test:unit": "vitest run src/**/*.test.{ts,tsx}",
    "test:integration": "vitest run src/**/*.integration.test.{ts,tsx}",
    "test:e2e": "vitest run src/__tests__/e2e/**/*.e2e.test.{ts,tsx}",
    "test:performance": "vitest run src/__tests__/performance/**/*.test.{ts,tsx}",
    "test:coverage": "vitest run --coverage",
    "test:coverage-html": "vitest run --coverage --reporter=html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@tanstack/react-query": "^4.24.0",
    "@types/leaflet": "^1.9.0",
    "@types/leaflet.markercluster": "^1.5.5",
    "@types/three": "^0.150.0",
    "axios": "^1.3.0",
    "date-fns": "^2.29.0",
    "leaflet": "^1.9.0",
    "leaflet.markercluster": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-leaflet": "^4.2.0",
    "react-leaflet-cluster": "^3.1.1",
    "react-router-dom": "^6.8.0",
    "socket.io-client": "^4.6.0",
    "three": "^0.150.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^26.1.0",
    "tsx": "^3.12.0",
    "typescript": "^5.0.0",
    "vite": "^4.1.0",
    "vite-plugin-pwa": "^0.14.0",
    "vitest": "^1.6.0"
  }
}
